{
  "name": "northrook/symfony-latte-bundle",
  "description": "Latte integration for Symfony.",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\Latte\\": "src"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
