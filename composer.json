{
  "name": "northrook/symfony-latte-bundle",
  "description": "Latte integration for Symfony.",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\Latte\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.2",
    "northrook/core": "dev-main",
    "northrook/logger": "dev-main",
    "latte/latte": "^3.0",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/deprecation-contracts": "^3.0",
    "symfony/http-kernel": "^7.0",
    "northrook/types": "dev-main",
    "northrook/support": "dev-main",
    "foroco/php-browser-detection": "^2.7",
    "symfony/filesystem": "^7",
    "northrook/elements": "dev-main",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "northrook/favicon": "dev-main"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/framework-bundle": "^7.0",
    "symfony/web-profiler-bundle": "^7",
    "symfony/stopwatch": "7",
    "symfony/security-bundle": "7.1.x-dev",
    "symfony/translation": "7.1.x-dev"
  }
}
